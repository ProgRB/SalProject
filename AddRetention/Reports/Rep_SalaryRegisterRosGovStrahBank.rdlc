  <Code>Public Function ToUpper(inputStr) as String
    If inputStr= ""  Then
      ToUpper = ""
    Else
      ToUpper = UCase(Left(inputStr, 1)) + LCase(Right(inputStr, len(inputStr) - 1))
    End If
    Exit Function
End Function

Public Function funSupr(xsu As Double) As String
On Error GoTo ersupr
If Not IsNumeric(xsu) Then
funSupr = ""
Exit Function
End If
If xsu &gt;= 10000000000000# Then
funSupr = "слишком большое число"
Exit Function
End If
Dim ssu As String, nsu, edi, des, sot, ind As Byte, i As Integer
If Fix(xsu) = 0 Then
funSupr = "ноль рублей "
Else
ssu = Mid$(str$(Fix(xsu)), 2)
nsu = (Len(ssu) + 2) \ 3
ssu = Right$("00", nsu * 3 - Len(ssu)) + ssu
For i = nsu To 1 Step -1
sot = Val(Mid$(ssu, (nsu - i) * 3 + 1, 1))
des = Val(Mid$(ssu, (nsu - i) * 3 + 2, 1))
edi = Val(Mid$(ssu, (nsu - i) * 3 + 3, 1))
If sot + des + edi &gt; 0 Or i = 1 Then
If sot &gt; 0 Then
funSupr = funSupr + Choose(sot, "сто", "двести", "триста", _
"четыреста", "пятьсот", "шестьсот", "семьсот", "восемьсот", _
"девятьсот") + " "
End If
If des = 1 Then
funSupr = funSupr + Choose(edi + 1, "десять", "одиннадцать", _
"двенадцать", "тринадцать", "четырнадцать", "пятнадцать", "шестнадцать", _
"семнадцать", "восемнадцать", "девятнадцать") + " "
ind = 3
Else
If des &lt;&gt; 0 Then
funSupr = funSupr + Choose(des - 1, "двадцать", _
"тридцать", "сорок", "пятьдесят", "шестьдесят", "семьдесят", "восемьдесят", _
"девяносто") + " "
End If
If edi &lt;&gt; 0 Then
If i = 2 And (edi = 1 Or edi = 2) Then
ind = 9
Else
ind = 0
End If
funSupr = funSupr + Choose(edi + ind, "один", "два", _
"три", "четыре", "пять", "шесть", "семь", "восемь", "девять", "одна", _
"две") + " "
End If
Select Case edi
Case 1
ind = 1
Case 2, 3, 4
ind = 2
Case Else
ind = 3
End Select
End If
funSupr = funSupr + Choose((i - 1) * 3 + ind, "рубль", "рубля", _
"рублей", "тысяча", "тысячи", "тысяч", "миллион", "миллиона", "миллионов", _
"миллиард", "миллиарда", "миллиардов", "триллион", "триллиона", _
"триллионов") + " "
End If
Next i
End If
ssu = Right$(Format$(xsu, "0.00"), 2)
des = Val(Left$(ssu, 1))
edi = Val(Right$(ssu, 1))
If des = 1 Then
ind = 3
Else
Select Case edi
Case 1
ind = 1
Case 2, 3, 4
ind = 2
Case Else
ind = 3
End Select
End If
funSupr = ToUpper(funSupr) + ssu + Choose(ind, " копейка", " копейки", " копеек")
Exit Function
ersupr:
funSupr = "ошибка"
End Function

Public Function PostTransfer(k as Double) as Double
	If k&lt;=1000 Then
		PostTransfer = k*0.05+40
		Exit Function
	Else
		If k&lt;=5000 Then
		    PostTransfer = k*0.04+50
		    Exit Function
		Else
			If k&lt;=20000 Then
			      PostTransfer = k*0.02+150
			      Exit Function
			Else
			       If k&lt;=500000 Then
				PostTransfer = k*0.015+250
				Exit Function
			       End If
			End If
		End If
	End If
	PostTransfer = 0		
End Function</Code>
